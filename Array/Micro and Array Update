#include<bits/stdc++.h>
using namespace std;

int main(){

    int t, n, k;
    cin>>t;

    while(t--){
        cin>>n>>k;
        int min = 1e7;
        for(int i=0; i<n; i++){
            int a; cin>>a;
            if(a<min)min = a;
        }

        k<min ? cout<<0<<"\n" : cout<<k-min<<"\n";
    }
    return 0;
}
