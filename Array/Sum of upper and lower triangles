class Solution
{   
    public:
    //Function to return sum of upper and lower triangles of a matrix.
    vector<int> sumTriangles(const vector<vector<int> >& matrix, int n)
    {
        int u = 0, l = 0, s = 0;
        for(int i=0; i<n; i++){
            for(int j=0; j<=i; j++) l += matrix[i][j];
        }
        
        //upper triangle
    for(int i=0; i<n; i++){
        for(int j=i; j<n; j++) u += matrix[i][j];
    }
    
    return {u, l};
    }
    
};
